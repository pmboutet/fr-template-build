<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SFFR_Total_Cost_Campaign_Content__c</fullName>
    <externalId>false</externalId>
    <formula>IF(NULLVALUE(SFFR_Unit_Cost__c, 0)==0,0,SFFR_Unit_Cost__c) + IF(NULLVALUE(SFFR_Total_Cost_Items__c,0)==0 , 0,SFFR_Total_Cost_Items__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Formula: total Amount of all Related Content Items plus the Cost of the Content Unit Cost.</inlineHelpText>
    <label>Total Campaign Content Cost</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
